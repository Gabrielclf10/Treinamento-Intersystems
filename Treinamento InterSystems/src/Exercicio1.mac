ROUTINE Exercicio1
    
    read !,"Digite o valor de inicio: ",x
    read !,"Digite o valor de final: ",y
    set count = ""

    write !
    for i=x:1:y{
        if i # 2 = 0 {
            write i_" é par",!

            if i # 10 = 0{
                do $Increment(count)
            }
        }    
    }
    write !,count_" números desse intervalo são multiplos de 10!",!